{
    "name": "frcho/backup-manager",
    "description": "A simple database backup manager for Symfony2 with support for S3, Rackspace, Dropbox, FTP, SFTP.",
    "keywords": ["database", "backup", "symfony", "S3", "Rackspace", "Dropbox", "FTP", "SFTP"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Fernando Granados",
            "email": "luisfer1g@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Frcho\\Bundle\\BackupManagerBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "~2.8|^3.0",
         "symfony/dependency-injection": "~2.3",
        "symfony/config": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/class-loader": "~2.2",
        "backup-manager/backup-manager": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-dropbox": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/flysystem-sftp": "^1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7"
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
   
}
